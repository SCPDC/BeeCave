  <DataSets>
    <DataSet Name="ds_EscrowBalanceSheet">
      <Query>
        <DataSourceName>MyPermitNow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ContractorID">
            <Value>=Parameters!ContractorID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--Starting Balance
Select
	Sort = 0,
	PaymentID = -1,
	RecievedDate = @Month+'/1/'+@Year,
	Description = 'Starting Balance',
	Credit = 0,
	Debit = 0,	
	Balance = (
			isnull((select
			sum(clpx.amount)
			from
			ContractorLicense_Payment_XREF clpx
			where
			clpx.isAddFund = 1
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.ReceivedDate &lt; @Month+'/1/'+@Year),0)
			-
			isnull((Select
			SUM(ptpx.amount)
			from
			PermitsTracked_Payment_XREF ptpx
			left outer join PermitsTracked pt on pt.PermitId = ptpx.PermitID
			left outer join PermitsTracked_Contact_XREF ptcx on ptcx.ContactID = ptpx.ContactID
			where
			isnull(ptpx.isVoid,0) = 0
			and ptpx.PaymentType like 'Escrow'
			and ptcx.ContractorID_Jurisdiction = @ContractorID
			and ptpx.ReceivedDate &lt; @Month+'/1/'+@Year),0)
			-
			isnull((select
			sum(clpx.amount)
			from
			ContractorLicense_Payment_XREF clpx
			where
			isnull(clpx.isAddFund,0) = 0
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.PaymentType like 'Escrow'
			and clpx.ReceivedDate &lt; @Month+'/1/'+@Year),0))
Union
--Permit Payments		
Select
	Sort = 1,
	PaymentID = ptpx.paymentID,
	RecievedDate = ptpx.receiveddate,
	Description = cast(pt.permitnumber as nvarchar)+' - '+ptpx.description,
	Credit = 0,
	Debit = ptpx.Amount	* -1,
	Balance = ptpx.Amount	* -1
	from
	PermitsTracked_Payment_XREF ptpx
	left outer join PermitsTracked pt on pt.PermitId = ptpx.PermitID
	left outer join PermitsTracked_Contact_XREF ptcx on ptcx.ContactID = ptpx.ContactID
	where
	isnull(ptpx.isVoid,0) = 0
	and ptpx.PaymentType like 'Escrow'
	and ptcx.ContractorID_Jurisdiction = @ContractorID
	and ptpx.ReceivedDate &gt;= @Month+'/1/'+@Year
	and ptpx.ReceivedDate &lt; dateadd(m,1,@Month+'/1/'+@Year)
Union
--Contractor Payments
Select
Sort = 1,
PaymentID = clpx.paymentId,
RecievedDate = clpx.ReceivedDate,
Description = clpx.Description,
Credit = 0,
Debit = clpx.Amount	* -1,
Balance = clpx.Amount * -1
from
			ContractorLicense_Payment_XREF clpx
			where
			isnull(clpx.isAddFund,0) = 0
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.PaymentType like 'Escrow'
			and clpx.ReceivedDate &gt;= @Month+'/1/'+@Year
			and clpx.ReceivedDate &lt; dateadd(m,1,@Month+'/1/'+@Year)
Union
--Add Escrow Funds

Select
Sort = 1,
PaymentID = clpx.PaymentId,
RecievedDate = clpx.ReceivedDate,
Description = case when clpx.Amount &gt; 0 then 'Funds Added to Escrow: ' when clpx.Amount &lt; 0 then 'Escrow Account Deduction: 'end +clpx.Description,
Credit = clpx.amount,
Debit = 0,
Balance = clpx.amount
from
			ContractorLicense_Payment_XREF clpx
			where
			clpx.isAddFund = 1
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.ReceivedDate &gt;= @Month+'/1/'+@Year
			and clpx.ReceivedDate &lt; dateadd(m,1,@Month+'/1/'+@Year)

Union
--Ending Balance
Select
Sort = 2,
PaymentID = -1,
ReceivedDate = dateadd(d, -1,(DATEADD(m,1,@Month+'/1/'+@Year))),
Description = 'Ending Balance',
Credit = 0,
Debit = 0,
Balance = ((
			isnull((select
			sum(clpx.amount)
			from
			ContractorLicense_Payment_XREF clpx
			where
			clpx.isAddFund = 1
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.ReceivedDate &lt; @Month+'/1/'+@Year),0)
			-
			isnull((Select
			SUM(ptpx.amount)
			from
			PermitsTracked_Payment_XREF ptpx
			left outer join PermitsTracked pt on pt.PermitId = ptpx.PermitID
			left outer join PermitsTracked_Contact_XREF ptcx on ptcx.ContactID = ptpx.ContactID
			where
			isnull(ptpx.isVoid,0) = 0
			and ptpx.PaymentType like 'Escrow'
			and ptcx.ContractorID_Jurisdiction = @ContractorID
			and ptpx.ReceivedDate &lt; @Month+'/1/'+@Year),0)
			-
			isnull((select
			sum(clpx.amount)
			from
			ContractorLicense_Payment_XREF clpx
			where
			isnull(clpx.isAddFund,0) = 0
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.PaymentType like 'Escrow'
			and clpx.ReceivedDate &lt; @Month+'/1/'+@Year),0))
			+
			isnull((select
			sum(clpx.amount)
			from
			ContractorLicense_Payment_XREF clpx
			where
			clpx.isAddFund = 1
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.ReceivedDate &gt;= @Month+'/1/'+@Year
			and clpx.ReceivedDate &lt; dateadd(m,1,@Month+'/1/'+@Year)),0)
			-
			isnull((Select
			SUM(ptpx.amount)
			from
			PermitsTracked_Payment_XREF ptpx
			left outer join PermitsTracked pt on pt.PermitId = ptpx.PermitID
			left outer join PermitsTracked_Contact_XREF ptcx on ptcx.ContactID = ptpx.ContactID
			where
			isnull(ptpx.isVoid,0) = 0
			and ptpx.PaymentType like 'Escrow'
			and ptcx.ContractorID_Jurisdiction = @ContractorID
			and ptpx.ReceivedDate &gt;= @Month+'/1/'+@Year
			and ptpx.ReceivedDate &lt; dateadd(m,1,@Month+'/1/'+@Year)),0)
			-
			isnull((select
			sum(clpx.amount)
			from
			ContractorLicense_Payment_XREF clpx
			where
			isnull(clpx.isAddFund,0) = 0
			and isnull(clpx.isVoid,0)=0
			and clpx.ContractorID = @ContractorID
			and clpx.PaymentType like 'Escrow'
			and clpx.ReceivedDate &gt;= @Month+'/1/'+@Year
			and clpx.ReceivedDate &lt; dateadd(m,1,@Month+'/1/'+@Year)),0))

Order by Sort, RecievedDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
