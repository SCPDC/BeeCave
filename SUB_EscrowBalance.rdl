    <DataSet Name="ds_ContractorInfo">
      <Query>
        <DataSourceName>MyPermitNow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ContractorID">
            <Value>=Parameters!ContractorID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select
Jurisdiction = p.parishname,
Name = case when len(ltrim(rtrim(clj.businessname)))&gt;0 and len(ltrim(rtrim(clj.FirstName)))&gt;0 then isnull(clj.BusinessName,'')+char(10)+'Attn: '+isnull(clj.FirstName,'')+' '+isnull(clj.LastName,'')
				when LEN(ltrim(rtrim(clj.businessname))) &gt; 0 and LEN(ltrim(rtrim(clj.firstname)))&lt;= 0 then isnull(clj.BusinessName,'')
				else isnull(clj.FirstName,'')+' '+isnull(clj.LastName,'') end,
--Name = case when len(ltrim(rtrim(clj.businessname))) &lt;= 0 then isnull(clj.FirstName,'')+' '+isnull(clj.LastName,'') else 'Attn: '+isnull(clj.FirstName,'')+' '+isnull(clj.LastName,'') end,
MailingAddress = isnull(clj.BusinessMailingAddress1,''),
MailingAddress2 = isnull(clj.BusinessMailingAddress2,''),
MailingCSZ = isnull(clj.BusinessMailingCity,'')+', '+isnull(clj.BusinessMailingState,'')+' '+isnull(cast(clj.BusinessMailingZipCode as nvarchar),'')
from
ContractorLicense_Jurisdiction clj
left outer join parish p on p.parishid = clj.jurisdictionID
where
clj.contractorID = @ContractorID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Jurisdiction">
          <DataField>Jurisdiction</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MailingAddress">
          <DataField>MailingAddress</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MailingAddress2">
          <DataField>MailingAddress2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MailingCSZ">
          <DataField>MailingCSZ</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ds_Logo">
