  <DataSources>
    <DataSource Name="MyPermitNow">
      <DataSourceReference>/Report Parts/Data Sources/dsMyPermitNow</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4a581d82-15bf-4552-b45a-757965eeb7a8</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="ds_Ponds">
      <Query>
        <DataSourceName>MyPermitNow</DataSourceName>
        <CommandText>Select
URL = 'https://www.mypermitnow.org/permit_details.aspx?permitid='+cast(pt.permitID as nvarchar),
ProjectNumber = pt.PermitNumber,
ProjectName = pt.CommercialBusinessName,
Address = ptax.Address+' '+ptax.City+' '+cast(ptax.Zip as nvarchar),
AddressDescription = ptax.Description,
DateValue = case when datepart(m,ptdfx.DateValue) = 5 then 'May'
				when datepart(m,ptdfx.DateValue) = 6 then 'June'
				when datepart(m,ptdfx.DateValue) = 7 then 'July'
				else '' end,
LastInspection = (select top 1 i.InspectionDateTime from WorkOrders wo left outer join inspections i on i.workorderID = wo.workorderID where wo.PermitID = pt.permitID order by i.InspectionDateTime desc)
from
PermitsTracked pt
left outer join PermitsTracked_Address_XREF ptax on ptax.PermitID = pt.PermitId
left outer join PermitsTracked_DateFields_XREF ptdfx on ptdfx.permitID = pt.permitID
where
pt.ParishId = 130
and pt.ProjectTypeID = 15
and pt.DesignationType like 'Pond'
and pt.isvoid = 0
order by ptdfx.DateValue</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="URL">
          <DataField>URL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProjectNumber">
          <DataField>ProjectNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Address">
          <DataField>Address</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProjectName">
          <DataField>ProjectName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AddressDescription">
          <DataField>AddressDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateValue">
          <DataField>DateValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastInspection">
          <DataField>LastInspection</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
