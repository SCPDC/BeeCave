  <DataSets>
    <DataSet Name="ds_Requirements">
      <Query>
        <DataSourceName>MyPermitNow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Requirement">
            <Value>=Parameters!Requirement.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
jrg.GroupName,
jrgx.Priority,
[Description] = Case when IsInspection = 0 then (select jprmx.Description from Jurisdiction_ProjectRequirement_Miscellaneous_XLU jprmx where jprmx.RequirementID = jrgx.RequirementID)
				Else (select itx.Description from InspectionsType_XLU itx where itx.InspectionTypeID = jrgx.RequirementID)
				End,
RequirementType = case when IsInspection = 0 then 'Requirement' else 'Inspection' End
from
Jurisdiction_Requirement_Groups_XREF jrgx
left outer join Jurisdiction_Requirement_Groups jrg on jrg.RequirementGroupID = jrgx.RequirementGroupID
Where
jrgx.RequirementGroupID in (@Requirement)
order by jrg.GroupName, jrgx.Priority</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="GroupName">
          <DataField>GroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="priority">
          <DataField>Priority</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RequirementType">
          <DataField>RequirementType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ds_Groups">
      <Query>
        <DataSourceName>MyPermitNow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProjectTypeID">
            <Value>=Parameters!ProjectTypeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
RequirementGroupID,
GroupName
from
Jurisdiction_Requirement_Groups
Where
JurisdictionID = 130
and ProjectTypeID = @ProjectTypeID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="RequirementGroupID">
          <DataField>RequirementGroupID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GroupName">
          <DataField>GroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ds_ProjecType">
      <Query>
        <DataSourceName>MyPermitNow</DataSourceName>
        <CommandText>select
ptptxl.Description,
ptptx.ProjectTypeID
from
PermitsTracked_ProjectType_XREF ptptx
left outer join PermitsTracked_ProjectType_XLU ptptxl on ptptxl.ProjectTypeID = ptptx.ProjectTypeID
where
JurisdictionID = 130</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProjectTypeID">
          <DataField>ProjectTypeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
