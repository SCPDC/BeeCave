  <DataSets>
    <DataSet Name="ds_PermitsbyStreet">
      <Query>
        <DataSourceName>MyPermitNow</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Start">
            <Value>=Parameters!Start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@End">
            <Value>=Parameters!End.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Street">
            <Value>=Parameters!Street.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subdivision">
            <Value>=Parameters!Subdivision.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
URL = 'https://www.mypermitnow.org/permit_details.aspx?permitid='+cast(pt.permitID as nvarchar),
PermitNumber = pt.PermitNumber,
PermitType = jsux.Description,
Address = ptax.Address,
Subdivision = pt.subdivision,
IssuedDate = pt.IssuedDate,
ClosedDate = ptshx.EnterInStatus
from
PermitsTracked pt
left outer join PermitsTracked_Address_XREF ptax on ptax.PermitID = pt.PermitId
left outer join Jurisdiction_SpecificUse_XLU jsux on jsux.SpecificUseID = pt.SpecificUseID
left outer join PermitsTracked_StatusHistory_XREF ptshx on ptshx.PermitID = pt.PermitId and ptshx.StatusID = 964
where
pt.ParishId = 130
and pt.ProjectTypeID = 3
and pt.IssuedDate &gt;= @Start
and pt.IssuedDate &lt;= @End
and ptax.Address like '%'+ISNULL(@Street,'')+'%'
and pt.subdivision like '%'+ISNULL(@Subdivision,'')+'%'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PermitNumber">
          <DataField>PermitNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="URL">
          <DataField>URL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PermitType">
